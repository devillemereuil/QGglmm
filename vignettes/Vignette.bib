	year = {2005},
	keywords = {Animals, Ankle, Body Weight, Computer Simulation, France, Likelihood Functions, Microsatellite Repeats, Models, Theoretical, Passeriformes, Pedigree, Quantitative Trait, Heritable, Research Design, Sample Size, Sexual Behavior, Animal},
	pages = {2839--2850},
	file = {Charmantier et Réale - 2005 - How do misassigned paternities affect the estimati.pdf:/home/flyos/.mozilla/firefox/e3zxcz18.default/zotero/storage/3H5SPEWW/Charmantier et Réale - 2005 - How do misassigned paternities affect the estimati.pdf:application/pdf}
}

