@article{boag_heritability_1983,
	title = {The heritability of external morphology in {Darwin}'s ground finches ({Geospiza}) on {Isla} {Daphne} {Major}, {Galapagos}},
	volume = {37},
	issn = {0014-3820},
	url = {http://www.jstor.org/stable/2408404},
	doi = {10.2307/2408404},
	abstract = {The repeatabilities of seven external morphological characters were high for Geospiza fortis and G. scandens on Isla Daphne Major, Galapagos, between 1975 and 1978. Geospiza fortis mates displayed positive assortative mating with respect to morphology in 1976, but not 1978, while scandens showed no consistent assortative mating in any year. Heritabilities in fortis were high in both years, averaging .76 based on three types of parent-offspring regression and on full sib intraclass correlations. Heritabilities in scandens were lower, averaging .46. 1978 heritabilities were complicated by uncertain paternity in some fortis families. Genetic correlations were also high among the seven characters in fortis, and had a multivariate structure like that seen in the phenotypic correlations. Geospiza fortis had a high heritability for PC1, representing overall body size (h2 = .75), while scandens had high heritabilities for PC2, representing bill "pointedness" (1.07), and PC3, representing a ratio between bill and non-bill characters (.64). As predicted by the heritabilities, fortis offspring showed a strong phenotypic response to natural selection of their parents in 1977. Geospiza fortis morphological variation may be influenced by introgression with G. fuliginosa and frequency dependent, temporally fluctuating directional selection, while G. scandens morphology may reflect an evolutionary history predominated by stabilizing selection.},
	number = {5},
	urldate = {2011-10-20},
	journal = {Evolution},
